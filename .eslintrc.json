{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    	"no-console": "off",
    	"no-debugger": "off",
    	"import/extensions": "off",
    	"import/no-mutable-exports": "off",
    	"class-methods-use-this": "off",
    	"max-classes-per-file": "off"
    }
}
