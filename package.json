{
  "name": "replapi-it",
  "version": "2.3.2",
  "description": "A NodeJS Wrapper allowing you access various places of information on Replit.",
  "type": "commonjs",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.js",
      "default": "./cjs/index.js"
    },
    "./module": "./esm/index.mjs",
    "./common": "./cjs/index.js"
  },
  "module": "./esm/index.mjs",
  "scripts": {
    "postinstall": "node -e \"try{require('./.scripts/postinstall')}catch(e){}\"",
    "start": "node esm/index.mjs",
    "test": "node tests/common.test.js --experimental-conditional-exports & node tests/esm.test.mjs --experimental-conditional-exports",
    "test:cjs": "node tests/common.test.js --experimental-conditional-exports",
    "test:esm": "node tests/esm.test.mjs --experimental-conditional-exports",
    "build:cjs": "rimraf cjs & babel esm --out-dir cjs",
    "postbuild:cjs": "node .scripts/postbuild:cjs.js",
    "lint": "eslint esm --ext .mjs",
    "lint:fix": "eslint esm --ext .mjs --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RayhanADev/REPLAPI.it.git"
  },
  "keywords": [
    "Replit",
    "GraphQL",
    "Restful",
    "Blog",
    "XML",
    "Cookies",
    "Login",
    "Parse",
    "Query",
    "Mutations",
    "API",
    "NodeJS"
  ],
  "author": "RayhanADev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/RayhanADev/REPLAPI.it/issues"
  },
  "homepage": "https://github.com/RayhanADev/REPLAPI.it#readme",
  "dependencies": {
    "atob": "^2.1.2",
    "core-js": "^3.11.1",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.1.3",
    "prettyjson": "^1.2.1",
    "regenerator-runtime": "^0.13.7",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-modules-umd": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@replit/database": "^2.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "2.2.1",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2"
  }
}
