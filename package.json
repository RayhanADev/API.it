{
  "name": "replapi-it",
  "version": "2.0.0",
  "description": "A wrapper for Repl.it's GraphQL (and soon REST and even Crosis) API. Designed to be easy to use!",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "test": "echo \"No Tests Specified\"",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./test,./images,./eslintrc.json,./replit,./CODE_OF_CONDUCT.md,./CONTRIBUTING.md,./LICENSE,./README.md,./.github --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RayhanADev/REPLAPI.it.git"
  },
  "keywords": [
    "Replit",
    "API",
    "NodeJS"
  ],
  "author": "RayhanADev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/RayhanADev/REPLAPI.it/issues"
  },
  "homepage": "https://github.com/RayhanADev/REPLAPI.it#readme",
  "dependencies": {
    "atob": "^2.1.2",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "rimraf": "^3.0.2"
  }
}
